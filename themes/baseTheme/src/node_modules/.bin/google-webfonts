#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/node" ]; then
  "$basedir//usr/bin/node"  --use_strict "$basedir/../gulp-google-webfonts/index.js" "$@"
  ret=$?
else 
  /usr/bin/node  --use_strict "$basedir/../gulp-google-webfonts/index.js" "$@"
  ret=$?
fi
exit $ret
